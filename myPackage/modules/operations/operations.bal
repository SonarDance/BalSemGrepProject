public function add(int num1, int num2) returns int {
    return num1 + num2;
}

public function subtract(int num1, int num2) returns int {
    return num1 - num2;
}

public function multiply(int num1, int num2) returns int {
    return num1 * num2;
}

public function divide(int num1, int num2) returns float {
    return <float>(num1 / num2);
}
